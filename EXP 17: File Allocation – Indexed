#include <stdio.h>

int main() {
    int n, i, j, index, b[20], no;
    printf("Enter number of files: ");
    scanf("%d", &n);
    for(i=0;i<n;i++) {
        printf("Enter index block: ");
        scanf("%d", &index);
        printf("Enter number of blocks: ");
        scanf("%d", &no);
        printf("Enter block numbers: ");
        for(j=0;j<no;j++) scanf("%d",&b[j]);
        printf("\nFile %d -> Index Block: %d\nBlocks: ", i+1, index);
        for(j=0;j<no;j++) printf("%d ", b[j]);
        printf("\n");
    }
    return 0;
}
